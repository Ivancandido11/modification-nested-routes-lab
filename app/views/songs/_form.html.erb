<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.hidden_field :artist_id %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.collection_select :artist_id, @artists, :id, :name, include_blank: "Select Artist" %>
  <br />

  <%= f.submit %>
<% end %>
